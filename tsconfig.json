{
  "compilerOptions": {
    // "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
  },
  "include": [
    "./**/*",
    "**/*",
    "*",
    "./*",
  ],
}